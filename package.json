{
  "name": "hello-world",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rm -rf ./dist",
    "compile": "npm run clean && gulp compile",
    "watch": "gulp watch",
    "lint": "eslint .",
    "html-lint": "gulp lint-all-html",
    "pretest": "npm run compile && eslint . && NODE_ENV=test npm run db-migrate && rm -rf test/screenshots/*.html",
    "test-mocha": "mocha",
    "test": "NODE_ENV=test npm run test-mocha && EXPRESS_BASE_PATH=/example-base-path NODE_ENV=test npm run test-mocha && npm run html-lint",
    "db-migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "heroku-postbuild": "npm run compile",
    "create-migration": "knex migrate:make --knexfile db/knexfile.js $MIGRATION"
  },
  "// NOTE": [
    "^Pinned dependencies are actually devDependencies, which are only",
    "included so that they are available in the Heroku build pipeline"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "bindings": "^1.2.1",
    "body-parser": "~1.17.1",
    "bookshelf": "^0.9.5",
    "browserify": "^13.0.1",
    "consolidate": "~0.14",
    "cookie-parser": "~1.3",
    "debug": "~2.2",
    "express": "~4.15.5",
    "font-awesome": "^4.7.0",
    "govuk-elements-sass": "^1.1.4",
    "govuk_frontend_toolkit": "^4.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.4.0",
    "gulp-debug": "^3.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^8.1.0",
    "gulp-rev-collector": "^1.2.3",
    "gulp-sass": "^2.3.1",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "helmet": "^3.5.0",
    "hogan.js": "~3.0",
    "i18n": "^0.8.2",
    "jquery": "^2.2.4",
    "knex": "^0.11.5",
    "lmt-utils": "~0.1.4",
    "morgan": "^1.6.1",
    "morgan-json": "^1.1.0",
    "node-uuid": "~1.4",
    "npm-git-secrets": "^1.0.1",
    "pg": "^4.5.5",
    "run-sequence": "^2.2.1",
    "serve-favicon": "~2.4.5",
    "setprototypeof": "^1.0.3",
    "vinyl-source-stream": "^1.1.0",
    "winston": "^2.2.0",
    "ws": "^3.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^3.0.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.4.5",
    "moment": "^2.14.1",
    "npm-git-secrets": "^1.0.4",
    "rev-del": "^1.0.5",
    "supertest": "^3.0.0",
    "zombie": "^4.2.1"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "5.2.0"
  }
}
